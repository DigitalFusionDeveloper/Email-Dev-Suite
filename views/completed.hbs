<h1>{{clientName}}</h1>


{{#if message}}
    <hr>
    <div class="row">
        <div class="col s6">
            <div class="card blue-grey">
                <div class="card-content white-text">
                    {{#each message}}
                        <p>{{this}}</p>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
{{/if}}
