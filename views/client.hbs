<h1>{{clientName}}</h1>
<div class="container">
  <div class="row">

  </div>
</div>
<div class="row">
    <form class="col s12" action="/folder/create/{{clientName}}" method="post">
        <div class="row">
            <div class="input-field col s8">
                <input placeholder="Includes: Data Extensions, My Emails, Portfolio, and User-Initiated" id="folderName" type="text" class="validate" name="folderName">
                <label for="folderName">Folder Name</label>
            </div>
            <button class="btn waves-effect waves-light" type="submit" name="action">Create Folders<i class="material-icons right">create_new_folder</i></button>
        </div>
    </form>
    <ul>
        {{#each message}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
</div>
